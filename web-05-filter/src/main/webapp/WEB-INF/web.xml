<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
    <filter>
        <filter-name>CharacterEncodingFilterTest</filter-name>
        <filter-class>top.novashen.filter.CharacterEncodingFilterTest</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CharacterEncodingFilterTest</filter-name>
        <url-pattern>/servlet/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>CharacterEncodingFilterTest</filter-name>
        <servlet-name>ShowServlet</servlet-name>
    </filter-mapping>

    <servlet>
        <servlet-name>ShowServlet</servlet-name>
        <servlet-class>top.novashen.servlet.ShowServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ShowServlet</servlet-name>
        <url-pattern>/servlet/s1</url-pattern>
    </servlet-mapping>
</web-app>
