<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                         http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">

    <context-param>
        <param-name>name</param-name>
        <param-value>value</param-value>
    </context-param>

    <servlet>
        <servlet-name>servletContest</servlet-name>
        <servlet-class>top.novashen.servlet.Servlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletContestTest</servlet-name>
        <servlet-class>top.novashen.servlet.ServletContextTest</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>servletDispatcher</servlet-name>
        <servlet-class>top.novashen.servlet.ServletDemo1</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>servletContest</servlet-name>
        <url-pattern>/context</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletContestTest</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>servletDispatcher</servlet-name>
        <url-pattern>/dispatcher</url-pattern>
    </servlet-mapping>
    
    
    <servlet>
        <servlet-name>fileDownLoadServlet</servlet-name>
        <servlet-class>top.novashen.servlet.FileDownLoadServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>fileDownLoadServlet</servlet-name>
        <url-pattern>/download</url-pattern>
    </servlet-mapping>
    
    
    
    
    
    
    
    


</web-app>